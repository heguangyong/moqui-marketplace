<?xml version="1.0" encoding="UTF-8"?>
<screen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/xml-screen-3.xsd"
        default-menu-title="智能供需平台" default-menu-index="1"
        include-child-content="true" require-authentication="false"
        menu-image="fa fa-shopping-cart" menu-image-type="icon">

    <subscreens default-item="Dashboard">
        <subscreens-item name="Dashboard" menu-title="控制台" location="component://moqui-marketplace/screen/marketplace/Dashboard.xml"/>
        <subscreens-item name="Supply" menu-title="供应管理" location="component://moqui-marketplace/screen/marketplace/Supply.xml"/>
        <subscreens-item name="Demand" menu-title="需求管理" location="component://moqui-marketplace/screen/marketplace/Demand.xml"/>
        <subscreens-item name="Matching" menu-title="智能匹配" location="component://moqui-marketplace/screen/marketplace/Matching.xml"/>
        <subscreens-item name="Chat" menu-title="AI助手" location="component://moqui-marketplace/screen/marketplace/Chat.xml"/>
        <subscreens-item name="TestDataInit" menu-title="数据管理" location="component://moqui-marketplace/screen/marketplace/TestDataInit.xml"/>
    </subscreens>

    <widgets>
        <subscreens-panel id="marketplace-panel" type="tab"/>
    </widgets>
</screen>